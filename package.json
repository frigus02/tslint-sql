    "docker:run": "docker run -it --rm -v /c/code/tslint-sql:/src --name tslint-sql-dev tslint-sql-dev",
    "docker:attach": "docker exec -it tslint-sql-dev bash",
    "test:rules": "tslint --test test/rules/**/tslint.json"
  }
}
